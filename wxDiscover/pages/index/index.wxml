<!--pages/index/index.wxml-->
<view class='container'>
	<view class='listGroup' wx:for='{{list}}' wx:for-item='group' wx:key='groupindex'>
		<view class='listItem' wx:for='{{group}}' wx:for-item='row' wx:key='rowindex'>
			<image src='{{row.icon}}'></image>
			<text>{{row.text}}</text>
			<image src='/images/arrow.png'></image>
		</view>
	</view>
</view>